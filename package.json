    "@types/react-dom": "^19.0.2",
    "astro": "^5.0.4",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-compiler-runtime": "^19.0.0-beta-55955c9-20241229",
    "react-dom": "^19.0.0",
    "rehype-external-links": "^3.0.0"
  },
  "devDependencies": {
    "@shikijs/colorized-brackets": "^1.24.4",
    "@shikijs/transformers": "^1.24.4",
    "@types/leaflet": "^1.9.15",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
