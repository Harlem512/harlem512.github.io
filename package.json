{
  "name": "harlem512.github.io",
  "description": "Harlem512's super cool website",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "d": "rm -r .astro && npm run dev",
    "build": "astro build",
    "preview": "astro build && astro preview",
